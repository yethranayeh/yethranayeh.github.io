{
	"name": "portfolio",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "npm@10.9.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:dev": "tsc && vite build --mode development",
		"preview": "vite preview",
